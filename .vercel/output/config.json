{
	"version": 3,
	"routes": [
		{
			"src": "^/blog$",
			"headers": {
				"Location": "/"
			},
			"status": 301
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [],
		"remotePatterns": []
	}
}